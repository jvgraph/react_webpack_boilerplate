{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "esversion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-console": 0,
    "no-debugger": 0,
    "no-set-state": "off",
    "no-undef": 0,
    "react/no-danger": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-no-target-blank": 0,
    "react/no-children-prop": 0,
    "strict": 0
  }
}